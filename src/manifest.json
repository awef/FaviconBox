{
  "name" : "FaviconBox",
  "version" : "0.5",
  "description" : "ブックマークバーの中身を表示します",
  "browser_action" : {
    "default_icon" : "icon_19x19.png",
    "default_popup" : "popup.html"
  },
  "icons" : {
    "16" : "icon_16x16.png",
    "48" : "icon_48x48.png",
    "128" : "icon_128x128.png"
